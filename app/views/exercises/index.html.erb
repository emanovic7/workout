<h1>Exercises</h1>

  <% @exercises.each do |exercise| %>
    <ol>
      <li><%= exercise.name %><li>
      <li><%= exercise.description %><li>
    </ol>
  <% end %>


<p>
  <%= link_to "load exercises", exercises_path(@exercises), :class => "load_exercises" %>
</p>


<div class="more-exercises">

</div>
